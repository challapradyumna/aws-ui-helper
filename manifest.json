{
    "name": "AWS UI Helper",
    "action": {
        "default_icon": {
        "16": "images/16x16.png",
        "24": "images/24x24.png",
        "32": "images/32x32.png"
        },
        "default_title": "AWS UI Helper"
    },
    "icons": {
        "16": "images/16x16.png",
        "32": "images/32x32.png",
        "48": "images/48x48.png",
        "128": "images/128x128.png"
      },
    "author": "Challa Pradyumna",
    "manifest_version": 3,
    "version": "0.1",
    "description": "Creates links for ALB, listeners and Target groups in Cloudformation",
    "content_scripts": [
        {
          "matches": ["https://*.console.aws.amazon.com/cloudformation/*"],
          "js": ["content-script.js"]
        }
      ],
    "background": {
      "service_worker": "background.js"
    }
  }